{
  "private": true,
  "name": "@scopedPackageName@",
  "version": "0.0.0",
  "author": "",
  "license": "",
  "description": "",
  "keywords": [],
  "homepage": "@homepageUrl@",
  "bugs": "@githubIssues@",
  "repository": {
    "type": "git",
    "url": "@githubRepository@"
  },
  "sideEffects": false,
  "main": "dist/@unscopedPackageName@.cjs",
  "module": "dist/@unscopedPackageName@.esm-bundler.mjs",
  "unpkg": "dist/@unscopedPackageName@.global.dev.js",
  "jsdelivr": "dist/@unscopedPackageName@.global.dev.js",
  "types": "dist/@unscopedPackageName@.d.ts",
  "exports": {
    ".": {
      "types": "./dist/@unscopedPackageName@.d.ts",
      "import": "./dist/@unscopedPackageName@.esm-bundler.mjs",
      "require": "./dist/@unscopedPackageName@.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^6.0.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.5",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "rimraf": "^5.0.1",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.2",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0",

    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-typescript": "^14.3.0",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "vite-plugin-vue-devtools": "^7.7.0"
  },
  "scripts": {
    "clean": "rimraf dist coverage",
    "clean:dist": "rimraf dist",
    "test:unit": "vitest --environment jsdom",
    "coverage": "vitest run --coverage --environment jsdom",
    "build": "run-s clean:dist build-only type-check",
    "build-only": "run-s build-dev build-neutral build-prod",
    "build-dev": "vite build --mode development",
    "build-neutral": "vite build --mode neutral",
    "build-prod": "vite build --mode production",
    "type-check": "run-p -c type-check:*",
    "type-check:code": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-check:config": "vue-tsc --noEmit -p tsconfig.node.json --composite false",
    "preinstall": "node ../../scripts/preinstall.js"
  }
}
